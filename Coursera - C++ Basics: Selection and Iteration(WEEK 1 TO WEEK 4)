Q3)
#include <iostream>
using namespace std;

int main(int argc, char** argv) {
  
  int a = stoi(argv[1]);
  int b = stoi(argv[2]);
  
  if (a > b) {
    int c = b;
    b = a;
    a = c;
  }
  
  //add code below this line
int sum = 0;
int iter = a;
  
while (iter <= b) {
  sum += iter;
  iter++;
}
  
if (a == b) {
  sum = a;
}
  
cout << sum << endl;



  //add code above this line
  
  return 0;
  
}
-----------------------------------------------------
Q4)
#include <iostream>
using namespace std;

int main() {

  for (int i = 100; i <= 100; i--) {
  if (i == 0) {
    cout << "Print me!" << endl;
    break;
  }
  else {
    while (true) {
      break;
      i++;
      cout << "Don't print me!" << endl;
    }
  }
}

  return 0;

}
