Q5)
#include <iostream>
using namespace std;

int main() {
  
  //add code below this line
for (int i = 1; i < 6; i++) {
  for (int j = 5 - i; j > 0; j--) {
    cout << ".";
  }
  cout << i << endl;
}


  //add code above this line
  
  return 0;
  
}
